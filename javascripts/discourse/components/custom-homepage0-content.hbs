{{#if this.isCustomHomepage}}
<section class="course-overview2">
<div id="calculatorexpress_com-calculator" style="text-align: center;">
    Loading...
</div>
<script src="https://firebasestorage.googleapis.com/v0/b/js-scripts-b71a4.appspot.com/o/scripts%2Fembed.js?alt=media&token=53c14d46-c103-4aa4-80f0-1d043988b399"></script>
<script type="text/javascript">
    var embed = '';
    document.getElementById('calculatorexpress_com-calculator').innerHTML = atob(embed);

    // Add a scrollbar to the iframe
    window.onload = function () {
        const iframe = document.querySelector('#calculatorexpress_com-calculator iframe');
        if (iframe) {
            iframe.style.width = '100%';
            iframe.style.height = '100%'; // Adjust height as needed
            iframe.style.overflow = 'auto'; // Enables scrolling
        }
    };
</script>
